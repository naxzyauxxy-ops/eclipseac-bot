# Discord bot token (from Discord Developer Portal)
BOT_TOKEN=your-bot-token-here

# Your Discord application's client ID
CLIENT_ID=your-client-id-here

# The Guild (server) ID where you want commands registered
GUILD_ID=your-guild-id-here

# Role ID that is allowed to manage licenses (leave blank to use ManageGuild perm instead)
ADMIN_ROLE_ID=your-admin-role-id-here

# URL of your license server
LICENSE_SERVER=http://localhost:3000

# Must match ADMIN_SECRET in the license server's .env
ADMIN_SECRET=change-me-to-something-secure
